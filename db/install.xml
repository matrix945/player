<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/player/db" VERSION="20130407" COMMENT="XMLDB file for local message plugin"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="local_player" COMMENT="each record is a VOD_ID and VOD_Name">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="accesskeyid" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="accesskeysecret" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
